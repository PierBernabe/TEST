myModule = angular.module('BlankApp', [])

myModule.controller('MonController', ['$scope','WebQuest',  function($scope) {
  $scope.coucou = function() { 
		console.log("Hello"); 
		$scope.toto="On pape nous ??";
		WebQuest.chercheCherche();
  }
}]);

myModule.factory('WebQuest', ['$http', function($http) {
  return {
    chercheCherche: function() {
      $http.get('http://localhost:3000/test')
      .then(res => {console.log('-->', res)});
    }
  }
}])
